{
  "name": "kedao",
  "version": "0.1.23",
  "description": "Rich Text Editor Based On Draft.js",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "banyudu",
  "files": [
    "src",
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "run-s -l clean build:js build:css",
    "build:css": "gulp css",
    "build:js": "ttsc",
    "build:watch": "run-p -l watch:*",
    "clean": "rimraf lib",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "run-s -l lint:fix-*",
    "lint:fix-js": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint:fix-style": "stylelint --fix src/**/*.{css,scss}",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "postversion": "git push --follow-tags && npm publish",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "npm run build",
    "version": "standard-version",
    "watch:css": "gulp watch",
    "watch:js": "ttsc -w"
  },
  "repository": "https://github.com/banyudu/kedao",
  "keywords": [
    "kedao",
    "draft",
    "draft-js",
    "rich",
    "text",
    "editor",
    "rich-text-editor",
    "wysiwyg"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/draft-js": "^0.11.8",
    "@types/node": "^17.0.13",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "draft-js-prism": "^1.0.6",
    "eslint": "^8.8.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prism-js": "^1.0.0",
    "prismjs": "^1.15.0",
    "react-recipes": "^1.4.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "standard-version": "^9.5.0",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-morph": "^13.0.3",
    "ts-node": "^10.4.0",
    "ts-rename-import-plugin": "^1.1.6",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@draft-js-plugins/mention": "^5.1.1",
    "@react-hookz/web": "^15.0.1",
    "draft-convert": "^2.1.8",
    "draft-js": "^0.11.5",
    "draft-js-multidecorators": "^1.0.0",
    "draftjs-utils": "^0.10.2",
    "immutable": "~4.0.0",
    "jotai": "^1.7.4",
    "merge-class-names": "^1.4.2",
    "prop-types": "^15.7.2",
    "react-color": "^2.14.1",
    "react-icons": "^4.3.1",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.{js,ts,tsx,jsx,css,md}": "prettier --write"
  }
}
